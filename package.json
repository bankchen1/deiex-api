{
  "name": "exchange",
  "version": "1.0.0",
  "description": "Cryptocurrency Trading Platform",
  "main": "dist/main.js",
  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "dev": "concurrently \"cd exchange-web && npm run dev\" \"cd exchange-api && npm run dev\" \"cd exchange-admin && npm run dev\"",
    "dev:web": "cd exchange-web && npm run dev",
    "dev:api": "cd exchange-api && npm run dev",
    "dev:admin": "cd exchange-admin && npm run dev",
    "install:all": "npm install && cd exchange-web && npm install && cd ../exchange-api && npm install && cd ../exchange-admin && npm install",
    "typeorm": "ts-node ./node_modules/typeorm/cli",
    "migration:create": "npm run typeorm -- migration:create",
    "migration:generate": "npm run typeorm -- migration:generate -d src/database/migrations",
    "migration:run": "npm run typeorm -- migration:run -d src/database/migrations",
    "migration:revert": "npm run typeorm -- migration:revert -d src/database/migrations",
    "seed:run": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed -n src/database/seeds/*.ts"
  },
  "keywords": [
    "cryptocurrency",
    "trading",
    "exchange"
  ],
  "author": "",
  "dependencies": {
    "@liaoliaots/nestjs-redis": "^10.0.0",
    "@nestjs-modules/ioredis": "^2.0.2",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.0.0",
    "@nestjs/event-emitter": "^2.1.1",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/platform-socket.io": "^10.0.0",
    "@nestjs/schedule": "^4.1.2",
    "@nestjs/swagger": "^7.1.0",
    "@nestjs/websockets": "^10.0.0",
    "@prisma/client": "^6.2.1",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "csv-writer": "^1.6.0",
    "date-fns": "^2.30.0",
    "ethers": "^6.13.5",
    "exceljs": "^4.4.0",
    "ioredis": "^5.4.2",
    "jest-mock-extended": "^4.0.0-beta1",
    "jsonwebtoken": "^9.0.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.3",
    "prom-client": "^15.1.3",
    "qrcode": "^1.5.4",
    "redis": "^4.6.10",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "socket.io": "^4.7.2",
    "speakeasy": "^2.0.0",
    "typeorm": "^0.3.20",
    "uuid": "^11.0.4",
    "ws": "^8.14.2",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.4.15",
    "@types/bcrypt": "^5.0.1",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.12",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^2.0.16",
    "@types/ws": "^8.5.8",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "concurrently": "^9.1.2",
    "eslint": "^8.42.0",
    "jest": "^29.7.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3"
  }
}
